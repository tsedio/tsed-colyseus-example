{
  "name": "tsed-colyseus-example",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@tsed/ajv": "^7.31.0",
    "@tsed/common": "^7.31.0",
    "@tsed/core": "^7.31.0",
    "@tsed/di": "^7.31.0",
    "@tsed/engines": "^7.31.0",
    "@tsed/exceptions": "^7.31.0",
    "@tsed/json-mapper": "^7.31.0",
    "@tsed/logger": "^6.6.2",
    "@tsed/logger-file": "^6.6.2",
    "@tsed/openspec": "^7.31.0",
    "@tsed/platform-cache": "^7.31.0",
    "@tsed/platform-exceptions": "^7.31.0",
    "@tsed/platform-express": "^7.31.0",
    "@tsed/platform-log-middleware": "^7.31.0",
    "@tsed/platform-middlewares": "^7.31.0",
    "@tsed/platform-params": "^7.31.0",
    "@tsed/platform-response-filter": "^7.31.0",
    "@tsed/platform-views": "^7.31.0",
    "@tsed/schema": "^7.31.0",
    "@tsed/swagger": "^7.31.0",
    "@colyseus/monitor": "^0.15.0",
    "@colyseus/playground": "^0.15.3",
    "@colyseus/tools": "^0.15.0",
    "colyseus": "^0.15.0",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.6.1",
    "@tsed/cli-plugin-jest": "4.6.1",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "yarn"
  }
}
